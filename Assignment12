		cout<<endl;
	cout<<"ST6 : ";
	st6.display();
     	cout<<endl;
	cout<<"ST7 : ";
	st7.display();

	cout<<endl;
	cout<<endl;
	cout<<endl;

	//cout<<endl<<endl;
	//cout<<"ST7 end : ";
	//st5.displayend();

	}



	void replacecardinstack(int a , int b)
	{

		if(a==1  &&  b==2)
		{
			a=st1.gettop()->getData();
			b=st2.gettop()->getData();
			if(st2.isEmpty()==true )
			{
				if(st1.gettop()->getData()==(0||13||26||39));
				{
					int c=st1.gettop()->getData();
					st2.push(c);
				    st1.pop();
				    st2.display();
				}

			}

			else if(st2.isEmpty()==false)
			{
				if(st2.gettop()->getData()==(0||13))
