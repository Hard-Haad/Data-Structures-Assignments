					if(Upst2.gettop()->getData() !=(0||13||26||39))
				     {

						if(Upst2.gettop()->getData()-st6.gettop()->getData()==1)

						{
						   if(st6.gettop()->getData()-Upst2.gettop()->getData()== -1)
							        {
							        	Upst2.push(n);
				                        st6.pop();

							        }


						}
						else
								{
									cout<<"wrong move2"<<endl<<endl;
								}
					  }
						else
								{
									cout<<"wrong move2"<<endl<<endl;
								}
					}

				}
				else
				{
					cout<<"wrong move"<<endl<<endl;
				}
			}
			else if(y==3)
			{
				if(Upst3.isEmpty()==true&&(n==12||n==25||n==38||n==51))
				{
					Upst3.push(n);
				    st6.pop();
				}
				else if(Upst3.isEmpty()==false)
				{


				  if(Upst3.displayend()->getData() == (12||25||38||51))
					{
					if(Upst3.gettop()->getData() !=(0||13||26||39))
				     {

						if(Upst3.gettop()->getData()-st6.gettop()->getData()==1)

						{
						   if(st6.gettop()->getData()-Upst3.gettop()->getData()== -1)
							        {
