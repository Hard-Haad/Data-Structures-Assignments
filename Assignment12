using std::cout;
using std::rand;
using namespace std;

class Node
{
    public:
            Node()
            {
                data = 0;
                next = NULL;
            }
            Node(int d)
            {
                data = d;
                next = NULL;
            }
            void setNext(Node *n)
            {
                next = n;
            }
            void setData(int n)
            {
                data = n;
            }
            int getData()
            {
                return data;
            }
            Node * getNext()
            {
                return next;
            }
            void display()
            {
                cout<<"["<<data<<"]"<<"  ";
            }
    private:
        int data;
        Node *next;
};




class Stack
{
    public:
            Stack()
            {
                top = NULL;
                end = NULL;
                size = 0;
            }
            bool isEmpty()
            {
                return (top == NULL );
            }
            void push(int d)
            {
                Node * n = new Node(d);
                if(isEmpty())
                {
                    end=n;
					top = n;
                }
                else
                {
                    n->setNext(top);
                    top = n;
                }
                size++;
            }
            void pop()
            {
                if(!isEmpty())
                {
                    Node * temp = top;
                    top = top->getNext();
                    temp->setNext(NULL);
                    delete temp;
                    temp = NULL;
                }
                else
                {
                    cout<<"Stack -- Already Empty !!!!"<<endl;
                }
                size--;
            }
            Node* gettop()
            {
            	Node * temp = top;
            	return temp;
            }
            Node* displayend()
            {
                 Node * xyz = end;
                 bool check = true ;
                 while(check==false )
                 {

                 }
                 return xyz;
            }
			void display()
            {
                 Node * temp = top;
                 while(temp != NULL)
                 {
                     temp->display();
                     temp = temp->getNext();

                 }
            }

    private:
        Node * top;
        Node * end;
        int size;
};






class Queue{
public:
	Queue()
	{
		front = NULL;
		rear = NULL;
		size = 0;
	}
	bool isEmpty()
	{
		return (rear == NULL && front==NULL);
	}


	void enquee(int d)
	{
		Node * n = new Node(d);
		if (isEmpty())
		{
			front = n;
			rear = n;
		}
		else
		{
			rear->setNext(n);
			rear = n;
		}
		size++;
	}

	int dequee()
			{
				if (!isEmpty())
				{
					Node * temp = front;
					front = front->getNext();
					temp->setNext(rear);

					if (temp == rear)
						rear->setNext(NULL);
						delete temp;
						temp = NULL;
						size--;


				}
				else
				{
					cout << "Queue -- Already Empty !" << endl;
				}

			}


	void display()
	{
		Node * temp = front;
		while (temp != NULL)
		{
			temp->display();
			temp = temp->getNext();
		}
	}
	void displayfront()
	{
		Node * temp = front;
		temp->display();
		temp=temp->getNext();
		temp->display();
		temp=temp->getNext();
		temp->display();


	}
	void next()
	{
		Node *n=front;
		//enquee(n);
		n=n->getNext();
		front=n;
		n->display();

	}
	Node* queuegetdata()
	{
		Node * temp = front;
        return temp;
	}
private:
	Node * front;
	Node * rear;
	int size;
};

class Deck
{

	private : bool *card;
	          int size;
	          Stack st1,st2,st3,st4,st5,st6,st7,Upst1,Upst2,Upst3,Upst4;
	          Queue q1;

	public : Deck()
	{
		this->size=52;
		this->card=NULL;
		card= new bool [size];
		init();
	}
	void init()
	{
		for(int i=0;i<52;i++)
		{
			card[i]=0;
	   }
	}
	bool checkarray()
	{
		int checkit=0;
			for(int i=0;i<52;i++)
		{
			if(card[i]==0)
			{
				checkit++;

			}

		}
		if(checkit==52)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
	void displaycard()
	{
		for (int i=0; i<52;i++)
		{
			cout<<card[i]<<endl;
		}
	}
	void passcards()
	{
		int checkpush=0;
		int randnumber;
		int counter=0;
		srand(time(NULL));

        //cout<<randnumber<<endl;
        while(counter<400)
        {
        	randnumber = (rand() % 52);
        	//cout<<randnumber<<endl;
        	if(card[randnumber]==0)
			{
				card[randnumber]=1;
				if(checkpush==0)
				{
					st1.push(randnumber);

				}
				else if(checkpush>=1&&checkpush<3)
				{
					st2.push(randnumber);
				}
				else if(checkpush>3&&checkpush<7)
				{
					st3.push(randnumber);
				}
				else if(checkpush>6&&checkpush<11)
				{
					st4.push(randnumber);
				}
				else if(checkpush>10&&checkpush<16)
				{
					st5.push(randnumber);
				}
				else if(checkpush>15&&checkpush<22)
				{
					st6.push(randnumber);
				}
				else if(checkpush>21&&checkpush<=28)
				{
					st7.push(randnumber);
				}
				else if(checkpush>=29&&checkpush<=52)
				{
					q1.enquee(randnumber);
				}
				checkpush++;

        	}
        	counter++;
        }
	}





	void display()
	{
	cout<<"Queue  "<<endl;
	q1.displayfront();
	cout<<endl;
	cout<<endl;
	cout<<endl;

	cout<<"UPST1 : ";
