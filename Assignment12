	}
	void next()
	{

		q1.next();
	}
    /*

    }*/

};

int main()
{
    	cout<<"Clubs  (0-12)  ;   Spades  (13-25)  ;   Diamond (26-38)  ;  Heart (39-52) "<<endl;
	cout<<endl;



	Deck d;

	d.passcards();
	cout<<endl;


	d.display();
	cout<<endl;
	cout<<endl;
	int sentinal=0;
	while(sentinal!=-1){
	int b=0;
	cout<<"press 1 to place card into other stack , 2 to place card in stack from queue ,"
	<<" 3  for next card in Queue and 4 to put card in upstack ( checking (replacecardinstack)  5) ";
	cin>>b;
	cout<<endl;
	if(b==1)
	{
	int t;
	int o;
	cout<<"enter stack no from which you'll pick card  : ";
	cin>>t;
	cout<<"enter stack no in which you'll put card  : ";
	cin>>o;
	cout<<endl;
	d.cardchange(t,o);
    }
    else if(b==2)
	{
	int stackno;
   	cout<<"enter stack no : " ;
   	cin>>stackno;
   	d.placefromqueue(stackno);
   	d.display();
    }
    else if(b==3)
	{
		d.next();
		d.display();
	}
    else if(b==4)
	{

   		int a,b;
    	cout<<"enter stack from which you will chose card : " ;
    	cin>>a;
    	cout<<"enter stack in which you will put card : ";
    	cin>>b;


    	d.replacecardinstack(a,b);
    	d.display();

    }
    else if(b==5)
    {

	int a,b=0;
	cout<<"enter stackno : ";
	cin>>a;
	cout<<"enter upsatckno : ";
	cin>>b;
   	d.placecardinupstack(a,b);
   	d.display();
   	cout<<endl;
    }
    cout<<"press -1 to terminate and any number to run and any other n0. to make next move :  "<<endl;
    cin>>sentinal;
    cout<<endl;
}
	getch();
	return 0;
}


