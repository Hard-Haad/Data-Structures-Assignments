	void placefromqueue(int stackno)
	{
		int qq=q1.queuegetdata()->getData();
		if(stackno==1)
		{
			st1.push(qq);
			q1.dequee();
		}
		else if(stackno==2)
		{
			st2.push(qq);
			q1.dequee();
		}
		else if(stackno==3)
		{
			st3.push(qq);
			q1.dequee();
		}
		else if(stackno==4)
		{
			st4.push(qq);
			q1.dequee();
		}
		else if(stackno==5)
		{
			st5.push(qq);
			q1.dequee();
		}
		else if(stackno==6)
		{
			st6.push(qq);
			q1.dequee();
		}
		else if(stackno==7)
		{
			st7.push(qq);
			q1.dequee();
		}

	}
	void placecardinupstack(int x, int y)
	{
		if (x==1)
		{
			int n=0;
			n=st1.gettop()->getData();
			if(y==1)
			{


				if(Upst1.isEmpty()==true&&(n==12||n==25||n==38||n==51))
				{
					Upst1.push(n);
				    st1.pop();
				}
				else if(Upst1.isEmpty()==false)
				{


				  if(Upst1.displayend()->getData() == (12||25||38||51))
					{
					if(Upst1.gettop()->getData() !=(0||13||26||39))
				     {

						if(Upst1.gettop()->getData()-st1.gettop()->getData()==1)

						{
						   if(st1.gettop()->getData()-Upst1.gettop()->getData()== -1)
