	}
	void cardchange(int c,int cc)
	{
		if(c==1)
        {

			int n=0;
			n=st1.gettop()->getData();
		//	cout<<n<<endl;
			if(cc==2)
			{

			st2.push(n);
			st1.pop();

			this->display();
			}
			else if(cc==3)
			{

			st3.push(n);
			st1.pop();

				this->display();
			}
			else if(cc==4)
			{

			st4.push(n);
			st1.pop();
				this->display();
			}
			else if(cc==5)
			{

			st5.push(n);
			st1.pop();
			this->display();
			}
			else if(cc==6)
			{

			st6.push(n);
			st1.pop();

			this->display();
			}
			else if(cc==7)
			{

			st7.push(n);
			st1.pop();

			this->display();
			}

		}


		else if(c==2)
		{
			int n=0;
			n=st2.gettop()->getData();
			cout<<n<<endl;
			if(cc==1)
			{

			st1.push(n);
			st2.pop();

			this->display();
			}
			else if(cc==3)
			{

			st3.push(n);
			st2.pop();

				this->display();
			}
			else if(cc==4)
			{

			st4.push(n);
			st2.pop();
				this->display();
			}
			else if(cc==5)
			{

			st5.push(n);
			st2.pop();
			this->display();
			}
			else if(cc==6)
			{

			st6.push(n);
			st2.pop();

			this->display();
			}
			else if(cc==7)
			{

			st7.push(n);
			st2.pop();

			this->display();
			}

		}
		else if(c==3)
	     {

			int n=0;
			n=st3.gettop()->getData();
			cout<<n<<endl;
			if(cc==1)
			{

			st1.push(n);
			st3.pop();

			this->display();
			}
			else if(cc==2)
			{

