			}
			else if(cc==7)
			{

			st7.push(n);
			st3.pop();

			this->display();
			}

		}



		else if(c==4)
		{
			int n=0;
			n=st4.gettop()->getData();
			cout<<n<<endl;
			if(cc==1)
